addNet semobj_neurocomp_S -i 6 -t 5 CONTINUOUS

addGroup visualinp 40 INPUT
addGroup actioninp 10 INPUT
addGroup praxisinp 10 INPUT
addGroup vercontextinp 10 INPUT
addGroup lexiconinp 58 INPUT

addGroup visualout 40 OUTPUT SUM_SQUARED -BIASED
addGroup actionout 10 OUTPUT SUM_SQUARED -BIASED
addGroup praxisout 10 OUTPUT SUM_SQUARED -BIASED
addGroup vercontextout 10 OUTPUT SUM_SQUARED -BIASED
addGroup lexiconout 58 OUTPUT SUM_SQUARED -BIASED

addGroup visualhid 20  -BIASED
addGroup actionhid 20 -BIASED
addGroup praxishid 20 -BIASED
addGroup vismotionhid 20 -BIASED
addGroup vercontexthid 20  -BIASED
addGroup lexiconhid 20 -BIASED
addGroup sem 30 -BIASED

connectGroups visualinp visualout -p ONE_TO_ONE -m 6.0 -r 0.0
connectGroups actioninp actionout -p ONE_TO_ONE -m 6.0 -r 0.0
connectGroups praxisinp praxisout -p ONE_TO_ONE -m 6.0 -r 0.0
connectGroups vercontextinp vercontextout -p ONE_TO_ONE -m 6.0 -r 0.0
connectGroups lexiconinp lexiconout -p ONE_TO_ONE -m 6.0 -r 0.0

connectGroups visualout visualhid -bi  -m 0 -r 0.1
connectGroups actionout actionhid -bi  -m 0 -r 0.1
connectGroups praxisout praxishid -bi  -m 0 -r 0.1
connectGroups vercontextout vercontexthid -bi  -m 0 -r 0.1
connectGroups lexiconout lexiconhid -bi -m 0 -r 0.1

connectGroups vismotionhid {visualhid actionhid} -bi -m 0 -r 0.1 -t parconn

for {set semi 0} {$semi < 30} {incr semi} {
    for {set visj 0} {$visj < 10} {incr visj} {
    connectUnits "sem:$semi" "visualhid:$visj" -type "semLat" -bi -m 0 -r 0.1
    }
    for {set visk 10} {$visk <20} {incr visk} {
    connectUnits "sem:$semi" "visualhid:$visk" -type "semMed" -bi  -m 0 -r 0.1                           
    }
}

connectGroups sem vismotionhid -bi -m 0 -r 0.1 -t semMTG  
connectGroups sem {vercontexthid lexiconhid} -bi  -m 0 -r 0.1 -t semSTG
connectGroups sem sem -m 0 -r 0.1 -t semconnw

connectGroups bias {sem visualhid vismotionhid actionhid praxishid vercontexthid lexiconhid visualout actionout praxisout vercontextout lexiconout} -m -3.0 -r 0

freezeWeights -g {sem visualhid vismotionhid actionhid praxishid vercontexthid lexiconhid visualout actionout praxisout vercontextout lexiconout} -t bias

source lang_procs.tcl
source my_procs.tcl

setObj visualout.initOutput 0.05
setObj actionout.initOutput 0.05
setObj praxisout.initOutput 0.05
setObj vercontextout.initOutput 0.05
setObj lexiconout.initOutput 0.05

setObj visualhid.initOutput 0.05
setObj vismotionhid.initOutput 0.05
setObj actionhid.initOutput 0.05
setObj praxishid.initOutput 0.05
setObj vercontexthid.initOutput 0.05
setObj lexiconhid.initOutput 0.05
setObj sem.initOutput 0.05
